    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "graphql": "^15.5.0",
    "ioredis": "^4.27.2",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.6.0",
    "pg": "^8.6.0",
